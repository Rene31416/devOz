    "typescript": "~5.3.3"
  },
  "dependencies": {
    "@aws-lambda-powertools/logger": "^2.13.1",
    "@aws-sdk/client-dynamodb": "^3.699.0",
    "@aws-sdk/client-ssm": "^3.744.0",
    "@aws-sdk/lib-dynamodb": "^3.744.0",
    "aws-cdk-lib": "^2.133.0",
    "aws-sdk": "^2.1692.0",
    "constructs": "^10.0.0",
    "inversify": "^6.1.4",
    "reflect-metadata": "^0.2.2",
